{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@": ["./src"],
      "$core/*": ["./src/core/*"],
      "$components/*": ["./src/components/*"],
      "$schemaRenderer/*": ["./src/schemaRenderer/*"],
      "$store/*": ["./src/store/*"],
      "$plugins/*": ["./src/plugins/*"],
      "$plugins": ["./src/plugins/index.ts"],
      "$types/*": ["./src/types/*"],
      "$utils/*": ["./src/utils/*"],
      "$assets/*": ["./src/assets/*"],
      "$public/*": ["../../public/*"],
      "$customSchema/*": ["./src/customSchema/*"]
    }
  },
  "include": ["src", "src/types"],
  "exclude": ["node_modules"]
}
