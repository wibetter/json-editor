{
  "name": "@wibetter/json-utils",
  "version": "7.0.0",
  "description": "提供JSON相关的各类工具方法，比如schema转json、json转schema、json元数据分析等",
  "keywords": [
    "json工具包",
    "schema2json",
    "json2schema",
    "getJsonDataByKeyRoute",
    "getSchemaByIndexRoute",
    "indexRoute2keyRoute"
  ],
  "author": "wibetter",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build2lib": "akfun build2lib && npm run build2esm",
    "build2esm": "akfun build2esm -n=index"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+http://git@github.com:wibetter/json-utils.git"
  },
  "bugs": {
    "url": "https://github.com/wibetter/json-utils/issues"
  },
  "dependencies": {
    "lodash": "^4.17.23",
    "qs": "^6.14.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/qs": "^6.9.12",
    "akfun": "^5.1.19"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  }
}
